{
  "root": true,
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "parser": "babel-eslint",
  "extends": "standard",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "generator-star-spacing": "error",
    "no-debugger": "error",
    "no-return-await": "off",
    "no-cond-assign": "off"
  }
}
