{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 7 versions", "not ie <= 8"]
      }
    }],
    "stage-2"
  ],
  "plugins": [
    "transform-export-extensions"
  ],
  "env": {
    "development": {
      "plugins": [
        "transform-runtime",
        "transform-export-extensions"
      ]
    },
    "test": {
      "plugins": [
        "transform-runtime",
        "transform-export-extensions",
        "transform-es2015-modules-commonjs"
      ]
    }
  }
}
